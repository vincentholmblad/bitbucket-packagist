{
    "name": "vincentholmblad/bitbucket-packagist",
    "description": "A simple package that automatically creates an includable composer file with all repositories of a bitbucket user or team.",
    "type": "library",
    "homepage": "https://github.com/vincentholmblad/bitbucket-packagist",
    "authors": [
        {
            "name": "Vincent Holmblad",
            "email": "hello@vincentholmblad.com",
            "homepage": "https://www.vincentholmblad.com"
        }
    ],
    "keywords": [
        "bitbucket", "packagist", "private"
    ],
    "version": "1.0",
    "license": "MIT",
    "config": {
    },
    "autoload": {
        "psr-0": {
            "BitbucketPackagist\\Builder": "",
            "BitbucketPackagist\\FileBuilder": ""
        }
    },
    "extra": {
        "merge-plugin": {
          "include": [
              "./bitbucket_packagist_base.json"
          ],
          "merge-scripts": true
        }
    },
    "require": {
        "gentle/bitbucket-api": "^1.1",
        "kriswallsmith/buzz": "^0.16",
        "composer/satis": "^1.0",
        "wikimedia/composer-merge-plugin": "^1.4"
    }
}
